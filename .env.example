# Frontend (Vite) environment variables
# Copy to .env or .env.production and fill the values. These are safe to expose in the client bundle.

# Public API base URL (your deployed Node server URL). Example: https://api.wandernest.xyz
VITE_API_URL=

# Force using the real backend in production builds
VITE_USE_BACKEND=true

# Disable client demo mode (use real API)
VITE_DEMO_MODE=false

# Optional: bypass auth in demos (do NOT use for real production)
VITE_BYPASS_AUTH=false
